{
    "name": "equinor-sdscoredev-components",
    "version": "2.30.9",
    "description": "Composite react components based on equinor design system (EDS)",
    "author": "John Wennstr√∏m <john.wennstrom@protonmail.com>",
    "license": "ISC",
    "main": "./dist/index.umd.js",
    "module": "./dist/index.es.js",
    "files": [
        "dist"
    ],
    "exports": {
        ".": {
            "import": "./dist/index.es.js",
            "require": "./dist/index.umd.js"
        }
    },
    "types": "./dist/index.d.ts",
    "homepage": "https://github.com/equinor/sdscoredev-components#readme",
    "repository": {
        "type": "git",
        "url": "https://github.com/equinor/sdscoredev-components.git"
    },
    "bugs": {
        "url": "https://github.com/equinor/sdscoredev-components/issues"
    },
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org/"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ],
        "overrides": [
            {
                "files": [
                    "**/*.stories.*"
                ],
                "rules": {
                    "import/no-anonymous-default-export": "off"
                }
            }
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "scripts": {
        "start": "vite build --watch",
        "build": "timeout 30s vite build; exit 0",
        "preview": "vite preview",
        "storybook": "start-storybook -p 6006",
        "storybook:build": "build-storybook -o docs",
        "release": "standard-version && git push --follow-tags && npm publish",
        "semantic-release": "semantic-release"
    },
    "devDependencies": {
        "@babel/core": "^7.19.1",
        "@rollup/plugin-commonjs": "^22.0.2",
        "@rollup/plugin-node-resolve": "^13.3.0",
        "@rollup/plugin-typescript": "^9.0.1",
        "@semantic-release/git": "^10.0.1",
        "@storybook/addon-docs": "^6.5.12",
        "@storybook/addon-essentials": "^6.5.12",
        "@storybook/addon-interactions": "^6.5.12",
        "@storybook/addon-links": "^6.5.12",
        "@storybook/addon-storysource": "^6.5.12",
        "@storybook/builder-webpack5": "^6.5.12",
        "@storybook/manager-webpack5": "^6.5.12",
        "@storybook/node-logger": "^6.5.12",
        "@storybook/preset-create-react-app": "^4.1.2",
        "@storybook/react": "^6.5.12",
        "@types/estree": "^1.0.0",
        "@types/styled-components": "^5.1.26",
        "babel-loader": "^8.2.5",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.2.1",
        "react": "17.0.2",
        "react-dom": "17.0.2",
        "react-router-dom": "^6.3.0",
        "rollup": "^2.79.1",
        "rollup-plugin-peer-deps-external": "^2.2.4",
        "rollup-plugin-terser": "^7.0.2",
        "rollup-plugin-typescript2": "^0.34.0",
        "semantic-release": "^19.0.5",
        "standard-version": "^9.5.0",
        "web-vitals": "^2.1.4",
        "webpack": "^5.74.0"
    },
    "peerDependencies": {
        "react": ">=16",
        "react-dom": ">=16",
        "react-router-dom": "6.4.3",
        "styled-components": ">=5"
    },
    "dependencies": {
        "@equinor/eds-core-react": "0.27.0",
        "@equinor/eds-icons": "^0.17.0",
        "@equinor/eds-tokens": "^0.9.0",
        "@types/react": "17.0.49",
        "@vitejs/plugin-react": "^2.1.0",
        "casual-browserify": "^1.5.19-2",
        "fast-sort": "^3.2.0",
        "object-merge-advanced": "^13.1.3",
        "odata-filter-builder": "^1.0.0",
        "odata-query": "^7.0.3",
        "react-select": "^5.4.0",
        "tsconfig-paths-webpack-plugin": "^3.5.2",
        "typescript": "^4.8.3",
        "vite": "^3.1.8",
        "vite-plugin-commonjs": "^0.5.2",
        "vite-plugin-dts": "^1.6.6"
    }
}
