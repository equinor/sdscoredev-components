{
  "name": "equinor-sdscoredev-components",
  "version": "0.0.0-semantic-release",
  "description": "Composite react components based on equinor design system (EDS)",
  "author": "John Wennstr√∏m <john.wennstrom@protonmail.com>",
  "license": "ISC",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/equinor/sdscoredev-components#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/equinor/sdscoredev-components.git"
  },
  "bugs": {
    "url": "https://github.com/equinor/sdscoredev-components/issues"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "scripts": {
    "start": "react-scripts start",
    "test": "react-scripts test",
    "build": "rollup -c",
    "watch": "rollup -c -w",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook -o docs",
    "release": "standard-version && git push --follow-tags && npm publish",
    "semantic-release": "semantic-release"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@rollup/plugin-commonjs": "^21.0.2",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@storybook/addon-actions": "^6.4.19",
    "@storybook/addon-essentials": "^6.4.19",
    "@storybook/addon-interactions": "^6.4.19",
    "@storybook/addon-links": "^6.4.19",
    "@storybook/builder-webpack5": "^6.4.19",
    "@storybook/manager-webpack5": "^6.4.19",
    "@storybook/node-logger": "^6.4.19",
    "@storybook/preset-create-react-app": "^4.0.1",
    "@storybook/react": "^6.4.19",
    "@storybook/testing-library": "^0.0.9",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.3",
    "@testing-library/user-event": "^13.5.0",
    "@types/styled-components": "^5.1.23",
    "babel-loader": "^8.2.3",
    "react": "^17.0.2",
    "react-router-dom": "^6.2.1",
    "rollup": "^2.68.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.31.2",
    "semantic-release": "^19.0.2",
    "standard-version": "^9.3.2",
    "web-vitals": "^2.1.4",
    "webpack": "^5.69.1"
  },
  "peerDependencies": {
    "@equinor/eds-core-react": ">=0.18.0",
    "react": ">=17.0.2",
    "react-dom": ">=17.0.2",
    "react-router-dom": ">=6.0.0",
    "styled-components": ">= 5"
  },
  "dependencies": {
    "@equinor/eds-core-react": ">=0.18.0",
    "@equinor/eds-icons": "^0.10.0",
    "@equinor/eds-tokens": "^0.7.0",
    "@storybook/addon-docs": "^6.4.19",
    "@storybook/addon-storysource": "^6.4.19",
    "@types/react": "^17.0.39",
    "casual-browserify": "^1.5.19-2",
    "object-merge-advanced": "^13.0.12",
    "odata-query": "^6.7.1",
    "react-scripts": "^5.0.0",
    "react-select": "^5.2.2",
    "styled-components": ">= 5",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "typescript": "^4.5.5"
  }
}
